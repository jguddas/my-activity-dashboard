    "d3-scale": "^3.2.1",
    "dayjs": "^1.8.27",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "leaflet": "^1.6.0",
    "localforage": "^1.7.4",
    "lodash": "^4.17.19",
    "node-sass": "^4.14.1",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-c3js": "^0.1.20",
    "react-dom": "^16.13.1",
